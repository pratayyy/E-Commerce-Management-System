<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ecom.pojo">
	<class name="Category" table="category">
		<id name="pkCategoryId" column="pk_category_id"
			type="java.lang.Integer">
			<generator class="increment" />
		</id>
		<property name="categoryName" column="category_name"
			type="string"></property>
		<property name="description" column="description"
			type="string"></property>
		<property name="isDeleted" column="is_deleted"
			type="java.lang.Integer"></property>
	</class>
</hibernate-mapping>